{
  "name": "vscode-find-pr",
  "displayName": "vscode-find-pr",
  "description": "A vscode plugin jumps to pull request url.",
  "version": "0.0.1",
  "publisher": "hotchemi",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": [
    "SCM Providers"
  ],
  "activationEvents": [
    "onCommand:find-pr.findPullRequest"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "find-pr.findPullRequest",
        "title": "Find Pull Request"
      }
    ],
    "keybindings": [
      {
        "command": "find-pr.findPullRequest",
        "key": "alt+f",
        "mac": "alt+f"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --fix --project tsconfig.json",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write --single-quote --trailing-comma all",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/lodash": "^4.14.104",
    "@types/node": "^7.0.43",
    "prettier": "1.7.4",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "lodash": "^4.17.5"
  }
}
